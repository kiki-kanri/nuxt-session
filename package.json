{
  "name": "@kikiutils/nuxt-session",
  "version": "3.1.2",
  "description": "Easy-to-use nuxt server-side session.",
  "license": "MIT",
  "author": "kiki-kanri",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kiki-kanri/nuxt-session.git"
  },
  "files": [
    "./dist"
  ],
  "scripts": {
    "build": "nuxt-module-build build",
    "bumplog": "changelogen --bump",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "prepack": "pnpm run build",
    "release": "pnpm run build && changelogen --push --release && npm publish"
  },
  "types": "./dist/types.d.ts",
  "dependencies": {
    "@kikiutils/nitro-session": "^2.0.1",
    "@nuxt/kit": "^3.14.1592"
  },
  "devDependencies": {
    "@kikiutils/changelogen": "^0.7.0",
    "@kikiutils/prettierrc": "^1.3.2",
    "@nuxt/module-builder": "^0.8.4",
    "nuxt": "^3.14.1592"
  },
  "keywords": [
    "nuxt",
    "nuxt session",
    "session"
  ],
  "engines": {
    "node": ">=18.12.1"
  },
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs",
      "types": "./dist/types.d.ts"
    }
  },
  "prettier": "@kikiutils/prettierrc"
}
